package kr.co.kwins.bookshop.pager;

public class Pager {
	private int page = 1;
	private int perPager = 10;
	
	public int getPage() {
		return page;
	}
	public void setPage(int page) {
		this.page = page;
	}
	public int getPerPager() {
		return perPager;
	}
	public void setPerPager(int perPager) {
		this.perPager = perPager;
	}
}
